class Personagem:
    def __init__(self, nome, classe, nivel, vida, mana, forca, agilidade, inteligencia, defesa, especial):
        self.nome = nome
        self.classe = classe
        self.nivel = nivel
        self.vida = vida
        self.mana = mana
        self.forca = forca
        self.agilidade = agilidade
        self.inteligencia = inteligencia
        self.defesa = defesa
        self.especial = especial

    def __str__(self):
        return (f"Personagem(nome={self.nome}, classe={self.classe}, nivel={self.nivel}, "
                f"vida={self.vida}, mana={self.mana}, forca={self.forca}, agilidade={self.agilidade}, "
                f"inteligencia={self.inteligencia}, defesa={self.defesa}, especial={self.especial})")


# Código sem Builder — extremamente verboso
p1 = Personagem(
    "Arthas",
    "Guerreiro",
    10,
    150,
    30,
    20,
    12,
    5,
    18,
    "Golpe Fatal"
)

print(p1)
